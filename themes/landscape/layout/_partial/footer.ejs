<script type="text/javascript">
  // footer fixed in minheight
  var winHeight = document.body.clientHeight || document.documentElement.clientHeight;
  var pageHeight = document.getElementById('outer').offsetHeight;
  //console.log(winHeight, pageHeight)
  if (winHeight > pageHeight) {
    document.body.className += 'short-page';
  }

</script>